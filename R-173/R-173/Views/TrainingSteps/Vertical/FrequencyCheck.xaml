        <CheckBox Style="{StaticResource 2frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 3frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 4frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 5frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 6frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 7frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 8frequencyCheck}" />
        <Control Template="{StaticResource ArrowBottom}" />
        <CheckBox Style="{StaticResource 9frequencyCheck}" />
    </StackPanel>
</UserControl>
